<p>Total Shipments sent via Mobile: {{shipments.length}}</p>
<p>Total line items: {{lineItems.length}}</p>
<table>
    <thead>
        <tr>
            <th>Order #</th>
            <th>Customer Name</th>
            <th>Customer Email</th>
            <th>Product Name</th>
            <th>Product Quantity</th>
        </tr>
    </thead>
    <tbody>
        

        {{#each lineItems}}
        
        <tr>
            <td>{{orderNumber}}</td>
            <td>{{recipientName}}</td>
            <td>{{customerEmail}}</td>
            <td>{{productName}}</td>
            <td>{{productQty}}</td>
            
            
        </tr>
        
        {{/each}}

    </tbody>
</table>